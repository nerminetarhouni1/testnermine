pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '5'))
    }
    stages {
        stage('Scan') {
            steps {
               withSonarQubeEnv('nermineQube') {
                    sh 'mvn clean compile org.sonarsource.scanner.maven:sonar-maven-plugin:4.0.0.4121:sonar -Dsonar.java.binaries=target/classes '
                    
                }
            }
        }
    }
}
